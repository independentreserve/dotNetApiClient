<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <PackageId>IndependentReserve.Client</PackageId>
    <Authors>Independent Reserve</Authors>
    <PackageIconUrl>https://www.independentreserve.com/img/ir-icon-logo-blue-50.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/independentreserve/dotNetApiClient</PackageProjectUrl>
    <RepositoryUrl>https://github.com/independentreserve/dotNetApiClient</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.4.1" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.1" />
    <PackageReference Include="Portable.BouncyCastle" Version="1.8.1.4" />
  </ItemGroup>

</Project>
